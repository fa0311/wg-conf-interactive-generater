{
  "name": "wg-conf-wizard",
  "version": "0.1.0",
  "description": "Interactive WireGuard config generator CLI",
  "type": "module",
  "bin": {
    "wg-conf-wizard": "dist/index.js"
  },
  "scripts": {
    "dev": "tsx src/index.ts",
    "build": "tsc -p tsconfig.json",
    "start": "node dist/index.js",
    "check": "tsc --noEmit",
    "lint": "biome check .",
    "format": "biome format --write .",
    "test": "vitest run"
  },
  "dependencies": {
    "@clack/prompts": "1.0.1",
    "@noble/curves": "2.0.1",
    "picocolors": "1.1.1",
    "zod": "4.3.6"
  },
  "devDependencies": {
    "@biomejs/biome": "2.4.2",
    "@types/node": "25.2.3",
    "tsx": "4.21.0",
    "typescript": "5.9.3",
    "vitest": "4.0.18"
  },
  "packageManager": "pnpm@10.30.0"
}
